{
  "name": "Socialet",
  "description": "Socialet",
  "version": "0.3.1",
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "icons": { 
    "16": "icons/ss16.png", 
    "48": "icons/ss64.png", 
    "128": "icons/ss128.png"
  },
  "background": {
    //"page": "html/background.html"
    "scripts": [
      "js/libs/jquery.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "html/*",
    "js/*",
    "css/*",
    "fonts/*"
  ],
  //"content_scripts": [
  //  {
  //    "css": ["css/bootstrap.min.css"],
  //    "js": ["js/libs/jquery.js", "myscript.js"]
  //  }
  //],
  "options_page": "html/config.html",
  "browser_action": {
    "default_icon": "icons/ss64.png",
    //"default_popup": "html/popup.html",
    "default_title": "Socialet"
  },
  "manifest_version": 2
}
